{
  "name": "Account federation step function",
  "id": "gentle-katydid-de85f5",
  "triggers": [
    {
      "event": "auth/account.created",
      "definition": {
        "format": "cue",
        "def": "file://events/auth-account-created.cue"
      }
    }
  ],
  "steps": {
    "add-to-intercom": {
      "id": "add-to-intercom",
      "path": "file://steps/add-to-intercom",
      "name": "Add to intercom",
      "runtime": { "type": "docker" },
      "after": [
        { "step": "$trigger" }
      ]
    },
    "send-to-slack": {
      "id": "send-to-slack",
      "path": "file://steps/send-to-slack",
      "name": "Send to slack",
      "runtime": { "type": "docker" },
      "after": [
        { "step": "$trigger" }
      ]
    }
  }
}
